{
  "name": "react-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3001 && node bin/www && node app.js",
    "server": "set PORT=3001 && nodemon bin/www && nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently  \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.14",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.4"
  }
}
